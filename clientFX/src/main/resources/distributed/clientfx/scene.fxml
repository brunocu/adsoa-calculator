<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="distributed.clientfx.FXMLController">
   <children>
      <TextArea fx:id="txtLog" editable="false" prefHeight="200.0" prefWidth="200.0" />
      <Separator prefWidth="200.0" />
      <HBox prefWidth="200.0">
         <children>
            <TextField fx:id="txtInput" prefHeight="25.0" prefWidth="165.0" />
            <Button fx:id="btnDo" mnemonicParsing="false" onAction="#sendMessage" text="Do" />
         </children>
      </HBox>
   </children>
</VBox>
